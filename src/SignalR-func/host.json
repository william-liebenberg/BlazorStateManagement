{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingExcludedTypes": "Request",
            "samplingSettings": {
                "isEnabled": true
            }
        }
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:01",
            "visibilityTimeout": "00:00:30",
            "batchSize": 32,
            "maxDequeueCount": 2,
            "newBatchThreshold": 16,
            "messageEncoding": "base64"
        }
    }
}